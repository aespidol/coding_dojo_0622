<h1>Log In</h1>
<form action="/users" method="post">
	<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
	<input type="text" name="Email">
	<input type="text" name="Password">
	<input type="submit" value="Log In">
</form>
<% if flash[:notice] %>
	<p>Invalid</p>
<% end %>